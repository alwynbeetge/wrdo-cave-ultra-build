
# Database Configuration
DATABASE_URL="postgresql://username:password@hostname:port/database"
DIRECT_URL="postgresql://username:password@hostname:port/database"

# Authentication & Security
NEXTAUTH_SECRET="your-nextauth-secret-key-here"
NEXTAUTH_URL="https://your-app-domain.railway.app"

# AI Integration
OPENAI_API_KEY="your-openai-api-key"
ANTHROPIC_API_KEY="your-anthropic-api-key"
WRDO_BRAIN_API_KEY="your-wrdo-brain-api-key"
WRDO_BRAIN_URL="https://api.wrdo.ai/brain"

# Email Integration (Gmail API)
GMAIL_CLIENT_ID="your-gmail-client-id"
GMAIL_CLIENT_SECRET="your-gmail-client-secret"
GMAIL_REFRESH_TOKEN="your-gmail-refresh-token"
GMAIL_ACCESS_TOKEN="your-gmail-access-token"

# WebSocket Configuration
WEBSOCKET_PORT="3001"
WEBSOCKET_CORS_ORIGIN="https://your-app-domain.railway.app"

# Queue System
REDIS_URL="redis://localhost:6379"
QUEUE_CONCURRENCY="5"
QUEUE_MAX_RETRIES="3"

# Application Configuration
PORT="3000"
NODE_ENV="production"
NEXT_TELEMETRY_DISABLED="1"

# External APIs
STRIPE_SECRET_KEY="your-stripe-secret-key"
STRIPE_PUBLISHABLE_KEY="your-stripe-publishable-key"
STRIPE_WEBHOOK_SECRET="your-stripe-webhook-secret"

# Monitoring & Analytics
SENTRY_DSN="your-sentry-dsn"
ANALYTICS_ID="your-analytics-id"

# File Storage
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="your-s3-bucket"
